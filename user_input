import java.io.*;
import java.util.*;
public class userInput {
    String name;
    String type;
    double price;
    boolean isImport;
    static int count;
    double sales_tax;
    public void setName(String name1)
    {
        name=name1;
    }
    public void setType(String type1)
    {
        type=type1;
    }
    public void setPrice(double price1) {
        price = price1;
    }
    public void setImport(boolean isImport1)
    {
        isImport=isImport1;
    }
    public void setCount(int count1)
    {
      count=count1;
    }
    public String getName()
    {
        return name;
    }
    public String getType()
    {
        return type;
    }
    public double getPrice()
    {
        return price;
    }
    public int getCount()
    {
        return count;
    }

}
